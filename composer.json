{
    "name": "tdm/swiftmailer-events-bundle",
    "type": "symfony-bundle",
    "description": "Links Symfony2 event handling with Swiftmailer.  Overrides Swiftmailer defaults",
    "keywords": ["swiftmailer", "symfony2", "events"],
    "homepage": "https://github.com/TDMobility/SwiftmailerEventsBundle.git",
    "license": "Apache2",
    "authors": [
        {
            "name": "Westin Pigott",
            "email": "wpigott@tdmobility.com"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "symfony/symfony": ">=2.3.0",
        "symfony/event-dispatcher": ">=2.3.0",
        "symfony/swiftmailer-bundle": ">=2.2.2"
    },
    "require-dev": {               
    },
    "autoload": {
        "psr-0": { "TDM\\SwiftMailerEventBundle": "" }
    },
    "minimum-stability": "dev",
    "target-dir": "TDM/SwiftMailerEventBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    }
}